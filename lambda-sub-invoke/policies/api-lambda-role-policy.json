{
  "Version": "2012-10-17",
  "Statement": [
      {
          "Sid": "Stmt1493060054000",
          "Effect": "Allow",
          "Action": [
              "lambda:InvokeAsync",
              "lambda:InvokeFunction"
          ],
          "Resource": [
              "arn:aws:lambda:*:*:*"
          ]
      },
      {
          "Sid": "Stmt1493060108000",
          "Effect": "Allow",
          "Action": [
              "logs:CreateLogGroup",
              "logs:CreateLogStream",
              "logs:DescribeLogStreams",
              "logs:PutLogEvents"
          ],
          "Resource": [
              "arn:aws:logs:*:*:*"
          ]
      }
  ]
}
